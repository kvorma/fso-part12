{
  "name": "todo-express-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "d:up": "cd .. && docker compose -f docker-compose.dev.yml --profile dev up",
    "d:down": "cd .. && docker compose -f docker-compose.dev.yml --profile dev down",
    "d:shell": "docker exec -it tododev-todoback-1 bash"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "debug": "~4.4.3",
    "express": "~5.2.1",
    "mongoose": "^9.2.1",
    "morgan": "~1.10.1",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}