{
  "name": "todo-express-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "d:up": "docker compose -f docker-compose-dev.yml up",
    "d:down": "docker compose -f docker-compose-dev.yml down",
    "d:shell": "docker exec -it XXX bash",
    "app:build": "docker compose -f docker-compose-build.yml build",
    "app:up": "docker compose -f docker-compose-build.yml up",
    "app:down": "docker compose -f docker-compose-build.yml down",
    "db:start": "docker compose -f docker-compose-dbs.yml up",
    "db:stop": "docker compose -f docker-compose-dbs.yml down"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "debug": "~4.4.3",
    "express": "~5.2.1",
    "mongoose": "^9.2.1",
    "morgan": "~1.10.1",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}