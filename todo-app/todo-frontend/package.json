{
  "name": "todo-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_BACKEND_URL=http://localhost:3000 vite",
    "build": "VITE_BACKEND_URL=http://localhost:3000 vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "d:up": "docker compose -f docker-compose-dev.yml up",
    "d:down": "docker compose -f docker-compose-dev.yml down",
    "d:shell": "docker exec -it todo-frontend-todoapp-dev-1 bash",
    "app:build": "docker compose -f docker-compose-build.yml build",
    "app:up": "docker compose -f docker-compose-build.yml up",
    "app:down": "docker compose -f docker-compose-build.yml down"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.0",
    "jsdom": "^28.1.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
